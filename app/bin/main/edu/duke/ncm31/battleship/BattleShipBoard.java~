package edu.duke.ncm31.battleship;

public class BattleShipBoard {
  private final int width;
  private final int height;

  public BattleShipBoard(int w, int h){
    this.width = w;
    this.height = h;
  }
  
  public int getWidth(){
    return width;
  }

  public int getHeight(){
    return height;
  }
}
